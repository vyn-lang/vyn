let @arr: [5]Int = [5, 3, 8, 1, 4]
let @n: Int = 5

stdout# arr

let @i: Int = 0

for when i < n {
    let @j: Int = 0

    for when j < n - i - 1 {
        if arr::j > arr::(j + 1) {
            let temp: Int = arr::j
            arr::j = arr::(j + 1)
            arr::(j + 1) = temp
        }

        j = j + 1
    }

    i = i + 1
}

stdout# arr
