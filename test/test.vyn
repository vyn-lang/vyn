static SIZE: Int = 10;

let @inp_arr: [SIZE]Int = [29, 12, 44, 9, 7, 97, 4, 33, 64, 35]
let @res_arr: [SIZE]Int

// Copy input to result
for let @i: Int from 0..SIZE {
    res_arr::i = inp_arr::i
}

// Bubble sort
for let @i: Int from 0..SIZE - 1 {
    let @swapped: Bool = false;
    for let @j: Int from 0..SIZE - i - 1 {
        if res_arr::j > res_arr::(j + 1) {
            let @temp: Int = res_arr::j;
            res_arr::j = res_arr::(j + 1);
            res_arr::(j + 1) = temp;
            swapped = true;
        }
    }
    if not swapped {
        break;
    }
}

stdout# inp_arr;
stdout# res_arr;
